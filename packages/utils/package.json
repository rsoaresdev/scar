{
  "name": "@scar/util",
  "version": "1.0.0",
  "description": "Shared utility functions for Scar",
  "exports": {
    "./functions/*": "./functions/*/index.js",
    "./embeds": "./embeds/index.js",
    "./database": "./database/index.js",
    "./json": "./json/index.js",
    "./images": "./images/index.js"
  },
  "type": "module",
  "scripts": {
    "format": "prettier . --write --cache --ignore-unknown",
    "format:check": "prettier . --check --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@scar/config": "workspace:*",
    "@scar/database": "workspace:*",
    "chalk": "5.3.0",
    "discord-api-types": "0.37.70",
    "discord.js": "14.14.1",
    "ms": "2.1.3"
  },
  "keywords": [
    "Scar",
    "util",
    "shared",
    "utility-functions"
  ],
  "author": "SirRamboia",
  "license": "MIT",
  "packageManager": "pnpm@8.15.3"
}
