  "lucide-react": "0.363.0",
  "mini-svg-data-uri": "1.4.4",
  "next": "14.1.4",
  "next-auth": "4.24.7",
  "next-nprogress-bar": "2.3.5",
  "react": "18.2.0",
  "react-colorful": "5.6.1",
  "react-dom": "18.2.0",
  "react-infinite-scroller": "1.2.6",
  "react-spring": "9.7.3",
  "react-table": "7.8.0",
  "react-wrap-balancer": "1.1.0",
  "recharts": "2.12.3",
  "server-only": "0.0.1",
  "sharp": "0.33.3",
  "sonner": "1.4.41",
  "validator": "13.11.0"
 },
 "devDependencies": {
  "@next/bundle-analyzer": "14.1.4",
