{
 "name": "@scar/dashboard",
 "version": "6.1.0",
 "description": "Dashboard for Scar built on Next.js and Tailwind.css",
 "scripts": {
  "start": "next start",
  "dev": "next dev",
  "build": "next build",
  "deploy": "pnpm run build && pnpm run start",
  "format": "pnpm prettier . --write --cache --ignore-unknown --plugin=prettier-plugin-tailwindcss",
  "format:check": "pnpm prettier . --check --cache",
  "analyze": "next build",
  "lint": "next lint",
  "lint:fix": "next lint --fix"
 },
 "dependencies": {
  "@headlessui/react": "1.7.18",
  "@headlessui/tailwindcss": "0.2.0",
  "@scar/config": "workspace:*",
  "@scar/database": "workspace:*",
  "@scar/util": "workspace:*",
  "@mdx-js/loader": "3.0.1",
  "@mdx-js/react": "3.0.1",
  "@next-auth/prisma-adapter": "1.0.7",
  "@next/mdx": "14.1.4",
  "@riskymh/discord-markdown": "3.0.0",
  "@tippyjs/react": "4.2.6",
  "@vercel/analytics": "1.2.2",
  "@vercel/speed-insights": "1.0.10",
  "class-variance-authority": "0.7.0",
  "clsx": "2.1.0",
  "cobe": "0.6.3",
  "discord-api-types": "0.37.76",
  "geist": "1.2.2",
  "globby": "14.0.1",
  "js-file-download": "0.4.12",
