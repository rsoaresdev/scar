  "eslint-config-next": "14.1.4",
  "postcss": "8.4.38",
  "prettier-plugin-tailwindcss": "0.5.12",
  "tailwind-gradient-mask-image": "1.2.0",
  "tailwind-merge": "2.2.2",
  "tailwindcss": "3.4.1",
  "tailwindcss-text-fill": "0.2.0"
 },
 "browserslist": [
  ">0.3%",
  "not dead",
  "not op_mini all"
 ],
 "keywords": [
  "Scar",
  "Dashboard",
  "React",
  "Next.js",
  "Tailwind.css",
  "web-dashboard"
 ],
